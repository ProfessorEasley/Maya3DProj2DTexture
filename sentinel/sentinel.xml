<?xml version="1.0"?>
<proj2tex>
    <projections>
        <projection>
            <name>frontProj</name>
            <direction>front</direction>
            <imagePath>front.png</imagePath>
        </projection>
        <projection>
            <name>sideProj</name>
            <direction>side</direction>
            <imagePath>side.png</imagePath>
        </projection>
        <projection>
            <name>backProj</name>
            <direction>back</direction>
            <imagePath>back.png</imagePath>
        </projection>
    </projections>
    <layers>
        <layer>
            <name>backLayer</name>
            <colorProjectionName>backProj</colorProjectionName>
            <transparencyProjectionName>sideProj</transparencyProjectionName>
        </layer>
        <layer>
            <name>frontLayer</name>
            <colorProjectionName>frontProj</colorProjectionName>
        </layer>
    </layers>
    <combinedImagePath>combined.png</combinedImagePath>
    <screenshotResolution>
        <width>1280</width>
        <height>720</height>
    </screenshotResolution>
    <bakedTextureResolution>
        <width>512</width>
        <height>512</height>
    </bakedTextureResolution>
</proj2tex>